{
    "name": "Python 3.12.9",
    "dockerFile": "Dockerfile",
    "remoteUser": "vscode",
    "customizations": {
        "vscode": {
            "settings": {
                "python.pythonPath": "/home/vscode/.pyenv/shims/python",
                "terminal.integrated.shellArgs": ["--login", "-b"]
            },
            "extensions": [
                "ms-python.python",
                "KevinRose.vsc-python-indent",
                "ms-python.vscode-pylance",
                "ms-toolsai.jupyter"
            ]
        }
    }
}
